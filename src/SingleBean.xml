<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd">
   
  <bean id="mathCheatObject" class="com.dependencyinjection.in.MathCheat"></bean> 
   
	<bean id="student" class="com.dependencyinjection.in.Student" >
		<property name="id" value="1001"></property>
		<property name="mathCheat" ref="mathCheatObject" />
	</bean>
	
	<bean id="astu" class="com.dependencyinjection.in.AnotherStudent" >
	<property name="mathCheat1" ref="mathCheatObject" />
	</bean>
</beans>